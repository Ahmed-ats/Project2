<div class="questionnaire-item uk-animation-slide-right" id="questionnnaire-item-online-profiles">
    <div class="question-title">
        <h2>Your developer profiles</h2>
    </div>
    <div class="question-container" id="socialType">
        <form class="uk-form-horizontal">
            <div class="uk-margin">
                <label class="uk-form-label" for="form-horizontal-text">GitHub</label>
                <div class="uk-form-controls">
                    <input class="uk-input" id="form-horizontal-text" type="text" placeholder="Your GitHub Link">
                </div>
            </div>
            <div class="uk-margin">
                <label class="uk-form-label" for="form-horizontal-text2">LinkedIn</label>
                <div class="uk-form-controls">
                    <input class="uk-input" id="form-horizontal-text2" type="text" placeholder="Your LinkedIn Link">
                </div>
            </div>
        </form>
        <div class="question-actions">
            <button id="social-link" class="uk-button uk-button-primary">Next</button>
        </div>
    </div>

</div>

<div class="questionnaire-item uk-animation-slide-right" id="questionnnaire-item-about">
    <div class="question-title">
        <h2>About You</h2>
    </div>
    <div class="question-container">
        <form>
            <div class="uk-margin">
                <label class="uk-form-label" for="about-you">About You</label>
                <textarea id="about-you" class="uk-textarea" rows="5" placeholder="Describe yourself so your fellow developers can get to know you..."></textarea>
            </div>
        </form>
    </div>
    <div class="question-actions">
        <button class="uk-button uk-button-primary" id="create-file">Create Profile</button>
    </div>
</div>