    <div class="questionnaire-item uk-animation-slide-right" id="questionnaire-item-idea">
        <div class="question-title">
            <h2>Application Concept. Select the answer that best describes your situation</h2>
        </div>
        <div class="question-container">
            <div class="uk-child-width-expand@s uk-text-center" uk-grid>
                <div>
                    <label for="have-idea">
                        <div data-state="notSelected" class="uk-card uk-card-default uk-card-body">
                            <input class="uk-radio hide-btn" type="radio" name="ideaType" value="haveIdea" id="have-idea">
                            You already have an idea and you want help to develop it
                        </div>
                    </label>
                </div>
                <div>
                    <label for="want-idea">
                        <div data-state="notSelected" class="uk-card uk-card-default uk-card-body">
                            <input class="uk-radio hide-btn" type="radio" name="ideaType" value="wantIdea" id="want-idea">
                            You want to contribute to an idea already in development
                        </div>
                    </label>
                </div>
                <div>
                    <label for="brainstorm-idea">
                        <div data-state="notSelected" class="uk-card uk-card-default uk-card-body">
                            <input class="uk-radio hide-btn" type="radio" name="ideaType" value="brainstormIdea" id="brainstorm-idea">
                            You want connect with a fellow developer and brainstorm an idea to develop
                        </div>
                    </label>
                </div>
            </div>
        </div>
        <div class="question-actions">
            <button class="uk-button uk-button-primary next-question" id="add-idea">Next</button>
        </div>
    </div>

    <div class="questionnaire-item uk-animation-slide-right" id="questionnaire-item-online-profiles">
        <div class="question-title">
            <h2>Your developer profiles</h2>
        </div>
        <div class="question-container" id="socialType">
            <form class="uk-form-horizontal">
                <div class="uk-margin">
                    <label class="uk-form-label" for="form-horizontal-text">GitHub</label>
                    <div class="uk-form-controls">
                        <input class="uk-input" id="form-horizontal-text" type="text" placeholder="Your GitHub Link">
                    </div>
                </div>
                <div class="uk-margin">
                    <label class="uk-form-label" for="form-horizontal-text2">LinkedIn</label>
                    <div class="uk-form-controls">
                        <input class="uk-input" id="form-horizontal-text2" type="text" placeholder="Your LinkedIn Link">
                    </div>
                </div>
            </form>
        </div>
        <div class="question-actions">
            <button class="uk-button uk-button-primary next-question" id="add-online-profiles">Next</button>
        </div>
    </div>

    <div class="questionnaire-item uk-animation-slide-right" id="questionnaire-item-about">
        <div class="question-title">
            <h2>About You</h2>
        </div>
        <div class="question-container">
            <form>
                <div class="uk-margin">
                    <label class="uk-form-label" for="about-you">Your Developer Story</label>
                    <textarea class="uk-textarea" id="about-you" rows="5" placeholder="Describe yourself so your fellow developers can get to know you..."></textarea>
                </div>
            </form>
        </div>
        <div class="question-actions">
            <button class="uk-button uk-button-primary" id="create-profile">Create Profile</button>
        </div>
    </div>
</div>